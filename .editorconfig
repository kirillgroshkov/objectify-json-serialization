# http://editorconfig.org

root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true

[**.md]
trim_trailing_whitespace = false

[**.java]
indent_size = 4

[**.xml]
indent_size = 2

[**.json]
indent_size = 2

[**.{yml,yaml}]
indent_size = 2
